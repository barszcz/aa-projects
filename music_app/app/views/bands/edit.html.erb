<h1>Edit band - <%= @band.name %></h1>
<form id="form_id" action="<%= band_url(@band) %>" method="post">
  <%= auth_token_input %>
  <%= method_input("patch") %>
  <label for="name">Band name:</label>
  <input type="text" name="band[name]" value="<%= @band.name %>">
  <input type="submit" value="Edit band!">
</form>
