<% @track.notes.each do |note| %>
  <%= "#{note.body} - #{note.user.email}" %>
  <% if current_user.id == note.user_id %>
    <form action="<%= note_url(note) %>" method="post">
      <%= auth_token_input %>
      <%= method_input("delete") %>
      <input type="submit" value="Delete Note">
    </form>
  <% end %>
<% end %>
